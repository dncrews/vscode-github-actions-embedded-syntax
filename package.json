{
  "name": "vscode-syntax-gha-github-script",
  "displayName": "GitHub Actions github-script Syntax",
  "description": "Embedded JavaScript syntax highlighting for actions/github-script in GitHub Actions YAML files.",
  "version": "0.0.1",
  "publisher": "local",
  "private": true,
  "engines": {
    "vscode": "^1.85.0"
  },
  "scripts": {
    "test": "node --test"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "scopeName": "gha.github-script.injection",
        "path": "./syntaxes/github-actions-github-script.injection.tmLanguage.json",
        "injectTo": [
          "source.yaml",
          "source.yaml.github-actions",
          "source.github-actions",
          "source.github-actions-workflow"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.javascript": "javascript"
        }
      }
    ]
  }
}
